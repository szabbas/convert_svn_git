		<class>com.newtglobal.eFmFmFleet.model.EFmFmZoneMasterPO</class>
		<class>com.newtglobal.eFmFmFleet.model.EFmFmUserMasterPO</class>
		<class>com.newtglobal.eFmFmFleet.model.EFmFmDeviceMasterPO</class>
		<class>com.newtglobal.eFmFmFleet.model.EFmFmVehicleCheckInPO</class>
		<class>com.newtglobal.eFmFmFleet.model.EFmFmVehicleMasterPO</class>
		<class>com.newtglobal.eFmFmFleet.model.EFmFmVendorMasterPO</class>
		<class>com.newtglobal.eFmFmFleet.model.EFmFmActualRoutTravelledPO</class>
