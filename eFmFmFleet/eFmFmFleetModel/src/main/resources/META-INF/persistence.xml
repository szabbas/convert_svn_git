		<class>com.newtglobal.eFmFmFleet.model.EFmFmClientMasterPO</class>
		<class>com.newtglobal.eFmFmFleet.model.EFmFmRouteAreaMappingPO</class>
		<class>com.newtglobal.eFmFmFleet.model.EFmFmClientRouteMappingPO</class>
		<class>com.newtglobal.eFmFmFleet.model.EFmFmClientUserRolePO</class>
