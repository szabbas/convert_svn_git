		<class>com.newtglobal.eFmFmFleet.model.EFmFmVendorContractInvoicePO</class>
		<class>com.newtglobal.eFmFmFleet.model.EFmFmFixedDistanceContractDetailPO</class>
		<class>com.newtglobal.eFmFmFleet.model.EFmFmTripBasedContractDetailPO</class>
		<class>com.newtglobal.eFmFmFleet.model.EFmFmTripAlertsPO</class>
